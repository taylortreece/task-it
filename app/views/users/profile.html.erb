<% if @edit_my_profile == false %>

<h1><%= @user.full_name %>'s Profile</h1>
<h4><%= @user.email %></h4>
<h4><%= @user.assigned_position.title %> - <%= @user.team.name %></h4>

<%= link_to "Edit Profile", "/profile/#{@user.id}/edit" %>

<% else %>

<%= render :partial => "users/edit_my_profile" %>

<% end %>

